let serial = ['003007', '003009', '003004', '001001', '001010', '001005',
'001004',
'001006',
'002022',
'002020',
'002023',
'002018',
'002015']
 currentMonth = () => {

    let x = window.location.pathname.split("/")
    // Month
    let currentPage = x[x.length - 1].toString().substr(2,2)
    let cloth = x[x.length - 2]
    let currentTitle = title[serial.indexOf(cloth)]
    // $("#currentTime").innerText = currentPage
    document.getElementById("currentTime").innerText = currentPage + ' 월'
    document.getElementById("currentTitle").innerText = currentTitle
    
    console.log(currentPage)
  }

  currentMonth();



